{
  "name": "frida-android-floating-menu",
  "version": "1.0.0",
  "description": "A floating menu for android games using frida",
  "scripts": {
    "docs": "typedoc",
    "lint": "tsc",
    "dev": "tsup -w",
    "build": "tsup"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "keywords": [
    "frida",
    "frida-gum",
    "lgl",
    "modmenu",
    "modding",
    "android",
    "frida-il2cpp-bridge"
  ],
  "author": "maarsalien",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/frida-gum": "^18.4.1",
    "tsup": "^7.2.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  }
}
